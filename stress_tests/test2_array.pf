// Test 2: Array operations
let start = Sys.time()
let arr = []
for i in 0...99999:
    arr.push(i)
end

let total = 0
for x in arr:
    total = total + x
end
let endTime = Sys.time()

Sys.print("Array length: " + arr.size())
Sys.print("Sum: " + total)
Sys.print("Time: " + (endTime - start) + " ms")
