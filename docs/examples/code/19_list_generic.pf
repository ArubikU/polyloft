// Test: Generic List
println("=== Generic List Test ===")

let numbers = List<Int>(1, 2, 3, 4, 5)
println("List size: " + numbers.size().toString())
println("First: " + numbers.get(0).toString())

numbers.add(6)
println("After add: " + numbers.size().toString())

let doubled = numbers.map((x: Int) -> Int => x * 2)
println("Doubled: " + doubled.toString())

println("âœ“ Generic List test passed")
